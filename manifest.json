{
  "manifest_version": 3,
  "name": "BackTab Navigator",
  "version": "1.0",
  "description": "Open links in background tabs for specified URLs",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}